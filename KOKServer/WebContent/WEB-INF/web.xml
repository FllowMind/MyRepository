  <servlet-mapping>
    <servlet-name>musicinfoservlet</servlet-name>
    <url-pattern>/musicinfoservlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>musicsetinfoservlet</servlet-name>
