  </servlet>
  <servlet-mapping>
    <servlet-name>musicsetinfoservlet</servlet-name>
    <url-pattern>/musicsetinfoservlet</url-pattern>
  </servlet-mapping>
  
    <servlet>
    <servlet-name>artistinfo</servlet-name>
